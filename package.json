{
  "name": "csscleaner",
  "displayName": "cssCleaner",
  "description": "Finds and highlights unused css in project",
  "version": "0.0.1",
  "repository": "https://github.com/T-Ev/csscleaner.git",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:file"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "csscleaner.clean",
        "title": "Clean CSS"
      },
      {
        "command": "csscleaner.onSave",
        "title": "Save CSS"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
